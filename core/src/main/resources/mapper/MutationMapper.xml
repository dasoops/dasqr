<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dasoops.dasq.core.dq.mapper.MutationMapper">

    <resultMap id="BaseResultMap" type="com.dasoops.dasq.core.dq.entity.po.Mutation">
        <id property="id" column="ID" jdbcType="BIGINT"/>
        <result property="name" column="NAME" jdbcType="VARCHAR"/>
        <result property="factor" column="FACTOR" jdbcType="VARCHAR"/>
        <result property="map" column="MAP" jdbcType="VARCHAR"/>
        <result property="score" column="SCORE" jdbcType="INTEGER"/>
        <result property="level" column="LEVEL" jdbcType="INTEGER"/>
        <result property="isDelete" column="IS_DELETE" jdbcType="VARCHAR"/>
        <result property="createUser" column="CREATE_USER" jdbcType="VARCHAR"/>
        <result property="createTime" column="CREATE_TIME" jdbcType="TIMESTAMP"/>
        <result property="updateUser" column="UPDATE_USER" jdbcType="VARCHAR"/>
        <result property="updateTime" column="UPDATE_TIME" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID
        ,NAME,FACTOR,
        MAP,SCORE,LEVEL,
        IS_DELETE,CREATE_USER,CREATE_TIME,
        UPDATE_USER,UPDATE_TIME
    </sql>
    <select id="selectListByKeyword" resultType="com.dasoops.dasq.core.dq.entity.po.Mutation">
        SELECT *
        FROM `TB_OTHER_SC2_MUTATION`
        Where NAME LIKE CONCAT('%', #{keyword}, '%')
    </select>
</mapper>
