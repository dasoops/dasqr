server:
  port: 4901

spring:
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql://127.0.0.1:3306/DASQR?serverTimezone=Asia/Shanghai&characterEncoding=UTF-8&useSSL=true&allowMultiQueries=true
    username: root
    password: root

mybatis-plus:
  global-config:
    db-config:
      # 全局逻辑删除的实体字段名
      logic-delete-field: isDelete

dasq:
  cq:
    core:
      consolePrintStack: true
      nativePrintStack: true
      loadLocalPluginList: false
      ws:
        url: "/ws/DasServer/"
        maxTextMessageBufferSize: 512000
        maxBinaryMessageBufferSize: 512000
        maxSessionIdleTimeout: 900000
        apiTimeout: 120000
      event:
        corePoolSize: 5
        maxPoolSize: 20
        keepAliveTime: 2000
        workQueueSize: 512