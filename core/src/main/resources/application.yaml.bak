server:
  port: 4701

spring:
  datasource:
    driver-class-name: com.mysql.cj.jdbc.Driver
    url: jdbc:mysql:///dasq?serverTimezone=Asia/Shanghai&characterEncoding=UTF-8&useSSL=true&allowMultiQueries=true
    username:
    password:
  redis:
    host:
    port: 6379
    database:
    password:

logging:
  level:
    com:
      dasoops:
        dasq:
          core: debug
  file:
    name: ../../logs/dasServer.log
  logback:
    rollingpolicy:
      max-file-size: 100MB
      max-history: 7
      total-size-cap: 1GB
      clean-history-on-start: true
  register-shutdown-hook: true

minio:
  enable: true
  url:
  port:
  bucket:
  accessKey:
  secretKey:
  tempDownloadPath: D:\temp

mybatis-plus:
  global-config:
    db-config:
      # 全局逻辑删除的实体字段名
      logic-delete-field: isDelete

dasq:
  # 控制台打印异常堆栈信息
  consolePrintStack: false
  # java端访问token
  token:
  # cqhttp url地址
  cqHttpUrl: http://127.0.0.1:4700
  # cq OAUTH2 access-token
  cqToken:
  # 开发群id
  devGroupId: 673745932
  # 主开发者id
  adminId: 776465218
  # 正式版
  isDemo: false
  # 命令样式 cool/normal
  style: cool
  # reboot sh 脚本路径
  rebootShellPath: /usr/local/gitRepo/dasServer/reboot.sh
