<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dasoops.dasserver.plugin.webManager.mapper.ConfigWebMapper">

    <resultMap id="BaseResultMap" type="com.dasoops.dasserver.cq.entity.dbo.ConfigDo">
        <id property="id" column="ID" jdbcType="INTEGER"/>
        <result property="keyword" column="KEYWORD" jdbcType="VARCHAR"/>
        <result property="value" column="VALUE" jdbcType="VARCHAR"/>
        <result property="description" column="DESCRIPTION" jdbcType="VARCHAR"/>
        <result property="canEdit" column="CAN_EDIT" jdbcType="INTEGER"/>
        <result property="isDelete" column="IS_DELETE" jdbcType="VARCHAR"/>
        <result property="createUser" column="CREATE_USER" jdbcType="VARCHAR"/>
        <result property="createTime" column="CREATE_TIME" jdbcType="TIMESTAMP"/>
        <result property="updateUser" column="UPDATE_USER" jdbcType="VARCHAR"/>
        <result property="updateTime" column="UPDATE_TIME" jdbcType="TIMESTAMP"/>
    </resultMap>

    <sql id="Base_Column_List">
        ID
        ,KEYWORD,`VALUE`,CAN_EDIT
        IS_DELETE,CREATE_USER,CREATE_TIME,
        UPDATE_USER,UPDATE_TIME
    </sql>
    <select id="selectMaxId" resultType="java.lang.Long">
        SELECT MAX(ID)
        FROM `TB_CORE_CONFIG`;
    </select>
</mapper>
